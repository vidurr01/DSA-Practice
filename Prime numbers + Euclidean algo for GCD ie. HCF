#include <bits/stdc++.h>
using namespace std;

void prime_numbers(int n){
    int counter = 0; int num = 2; 
    while (counter<n){
        bool flag = false;
        for(int i = 2; i*i<=num;i++){
            if (num%i==0) {
                flag = true;
                break;}
        }
        
        if (flag == false) {
            cout<<num<<endl; num++; counter++;
        }
        else num++;
    }
}

int GCD_using_euclidean_algo(int n1, int n2){
    if (n1 == 0) return n2;
    if (n2 == 0) return n1;
    return GCD_using_euclidean_algo(max(n1,n2)%min(n1,n2),min(n1,n2));
    
    
}
int main(){
    cout<<"How many prime numbers do you want: "; 
    int n; 
    cin>>n;
    prime_numbers(n);
    cout<<endl;
    
    cout<<"Enter two numbers: "; int n1,n2; cin>>n1>>n2;
    cout<<"GCD = "<<GCD_using_euclidean_algo(n1,n2);
    
    return 0;
}
